# -*- mode: snippet -*-
# name: run
# key: run
# --
#!/bin/bash
set -e
export TZ="Asia/Shanghai"
ps -ef | grep app | awk '{print $2}' | xargs kill -9
mvnj11 clean package -U -DskipTests

$JAVA_11_ARM_HOME/bin/java \
-Xms124m -Xmx512m \
-agentpath:/Users/van/ZY/workspace/jrebel/lib/libjrebel64.dylib \
--spring.profiles.active=local \
--spring.cloud.nacos.config.enabled=false \
--spring.cloud.nacos.discovery.enabled=false \
-Dlogging.level.com.baomidou.mybatisplus=DEBUG \
-Dmybatis-plus.configuration.log-impl=org.apache.ibatis.logging.stdout.StdOutImpl \
-jar target/app-1.0.0-SNAPSHOT.jar